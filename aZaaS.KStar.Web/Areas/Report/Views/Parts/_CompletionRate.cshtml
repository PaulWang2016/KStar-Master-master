@{
    ViewBag.Title = "流程处理完成率";
}
@*<script type="text/javascript" src="/Scripts/jquery-1.9.0.min.js"></script>*@
<script type="text/javascript" src="/JS/highcharts-4.0.3/js/highcharts.js"></script>
<script type="text/javascript" src="/JS/highcharts-4.0.3/js/highcharts-more.js"></script>
<script src="~/Scripts/kstarform.userpick.js"></script>
<style type="text/css">
    .selectbar1 .texttd {
        float:left;
        width:240px;
    }
    .selectbar1 .tdbtn {
        float: left;
        width: 60px;
    }
    .selectbar1 .texttd div {float: left;}
    .selectbar1 .texttd .texttd-label { line-height: 26px;}
    #statusGroup .k-grid-content {
        overflow-x: hidden;
        overflow-y: scroll;
    }
    #ProcInstListGrid .k-grid-content {
        overflow-x: hidden;
        overflow-y: scroll;
    }
    .k-multiselect-wrap {
        width:100%;
    }
</style>
<div>
    
    <h3 style="text-align:center;">
        <b>流程处理完成率</b>
    </h3>
</div>
<div class="section k-toolbar k-grid-toolbar" style="background-color: #f5f5f5; margin-bottom:20px">
    <div class="selectbar1">
        <table>
            <tr>
                <td>
                    <div class="texttd">
                        开始时间:
                        <span>
                            <input class="datepicker" name="txtStartDate" type="text" id="txtStartDate" />
                        </span>
                    </div>
                    <div class="texttd">
                        结束时间:
                        <span>
                            <input class="datepicker" name="txtFinishDate" type="text" id="txtFinishDate" />
                        </span>
                    </div>
                    <div class="category-label texttd">
                        归属:
                        <span>
                            <input class="k-textbox" name="ProcessCategory" type="text" id="ProcessCategory" />
                        </span>
                    </div>
                    <div class=" texttd">
                        <div class="texttd-label">部门/科室:</div>
                        <div data-control='userpick' style="width:175px;" data-target='txtDeptName' data-options="{mutilselect:false}" data-controltype='Department' class="col-sm-5 has-feedback">
                            <input type="text"
                                   class="form-control" id="txtDeptName" style="height: 28px;" data-bind="value: txtDeptName" placeholder="请选择部门/科室" />
                            <span class="glyphicon glyphicon-search form-control-feedback" style="text-align:left;" id="txtDeptName1"></span>
                        </div>
                    </div>
                    <div class=" texttd">
                        <div class="texttd-label">发起人:</div>
                        @*<span>
                <input class="k-textbox" name="startPerson" type="text" id="startPerson" />
            </span>*@
                        <span id="spanPerson" style="width:193px;" data-control='userpick' data-target='startPerson' data-callback="intStartPerson" data-options="{mutilselect:false}" data-controltype='Person' class="col-sm-5 has-feedback ">
                            <input type="text"
                                   class="form-control" id="startPerson" style="height: 28px;" data-bind="value: startPerson" placeholder="点击选择" />
                            <span class="glyphicon glyphicon-search form-control-feedback" style="text-align:left;" id="FAECC2"></span>
                        </span>
                    </div>
                </td>
            </tr>
            <tr>
                <td>
                    <div class=" texttd" style="width:480px">
                        @*流&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;程:
                        <span>
                            <input class="k-textbox" name="stProcessName" type="text" id="stProcessName" data-placeholder="请选择流程...." />
                        </span>*@
                        <div class="texttd-label">
                            流程名称:
                        </div>
                            <select id="stProcessName" style="width:400px; vertical-align: middle; float:left"></select>
                        </div>
                        <div class="tdbtn">
                            <span>
                                <input class="k-button" id="btnQuery" type="button" value="查询" />
                            </span>
                        </div>
                        <div class="tdbtn">
                            <span>
                                <input class="k-button" id="btnReset" type="button" value="重置" />
                            </span>
                        </div>
                </td>
            </tr>
        </table>
    </div>
</div>
<div class="sectionGrid k-grid k-widget k-secondary" style="margin: 0px auto; width: 100%;">
    <input type="hidden" id="hidStatus" />
    <div id="container1" style="min-width:500px;height:400px"></div>
    <div id="statusGroup" style="width:200px;height:210px;position:absolute;z-index:1000; right:50px;bottom:50px;"></div>
</div>
<div id="ProcInstListWindow" style="display:none;">
    <div id="ProcInstListGrid" style="width:100%;">
    </div>
</div>
<script src="/JSResource/GetJavascriptResx?jsPageName=Report_SeaProcDealDuration" type="text/javascript"></script>
<script src="~/JS/seajs/sea.js"></script>
<script type="text/javascript">
    seajs.use("/JS/Report/SeaCompletionRate", function (init) {
        init();
    });

</script>



