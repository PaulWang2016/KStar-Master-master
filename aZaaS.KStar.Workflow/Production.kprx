            {
                K2.SetStartRule(K2.Configuration.StartDays, K2.Configuration.StartHours, K2.Configuration.StartMinutes, K2.Configuration.StartSeconds, K2.Configuration.Zone);
            }
            _startRuleSet = true;
        }

