                    propName = string.Format("ReturnPropertyValueXmlFieldPath{0}", setIdx);
                    break;
            }


            System.Reflection.PropertyInfo pi = K2.Configuration.GetType().GetProperty(propName);
            if (pi == null)
                return string.Empty;

