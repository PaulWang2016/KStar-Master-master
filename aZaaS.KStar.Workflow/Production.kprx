            int EscalateDays = K2.Configuration.EscalateDays;
            int EscalateHours = K2.Configuration.EscalateHours;
            int EscalateMinutes = K2.Configuration.EscalateMinutes;
            int EscalateSeconds = K2.Configuration.EscalateSeconds;
