                    {
                        XmlSchemaSet set = new XmlSchemaSet();

                        XmlDocument xmldoc = new XmlDocument();
                        xmldoc.LoadXml(this.SchemaUri);

                        if(xmldoc.SelectNodes("Schemas/Schema").Count > 0)
                        {
