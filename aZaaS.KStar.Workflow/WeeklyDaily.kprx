                    foreach (XMLHelper_XMLField item in this)
                    {
                        if ((String.Compare(item.Name, name, true) == 0) && item.Scope == scope)
                        {
                            return item;
                        }
                    }
                    return null;
                }
            }
            public bool Contains(string name, K2Scope scope)
            {
                if (this[name, scope] != null)
                {
                    return true;
                }
                return false;
            }
            #endregion
        }

        public class XMLHelper_XMLField
        {
