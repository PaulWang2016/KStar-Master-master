            public XMLHelper_XMLField(string name, K2Scope scope, string xmlValue, string schemaUri)
                : this()
            {
                this.Name = name;
                this.XMLValue = xmlValue;
                this.SchemaUri = schemaUri;
                this.Scope = scope;
