                K2.ProcessInstance.Logger.LogDebugMessage("Outcome.SucceedingRule.Helper", "Outcome Evaluation: Completed");
				return retval;
			}

			public static void SetOutcomeResult(Project_362fffaa2b9d4bf1ad2708867581c80e.SucceedingRuleContext_ab08aab57523405ba6d3546c6b33b92a K2, string value)
			{
