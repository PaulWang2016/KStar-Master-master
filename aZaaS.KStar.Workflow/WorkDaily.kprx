	public partial class SucceedingRule_ab08aab57523405ba6d3546c6b33b92a : IWorkflowContext<hostContext>
	{
		#region K2 Context

			private hostContext _k2;

			#endregion

		#region IWorkflowContext<hostContext> Members

			public Project_362fffaa2b9d4bf1ad2708867581c80e.SucceedingRuleContext_ab08aab57523405ba6d3546c6b33b92a K2
			{
					get
					{
						return _k2;
					}
					set
					{
						_k2 = value;
					}
			}

			#endregion

		private void EvaluateRule(object sender, ConditionalEventArgs e)
			{

					e.Result = SucceedingRuleHelper.AnyOutcomesEvaluatedSuccessfully(K2);
			 
			}

			private void SetSucceedingRuleTrue_ExecuteCode(object sender, EventArgs e)
			{
					K2.SucceedingRule = true;
					var ignoringPeekPSetActions = string.Empty;
					bool hasUnpeekedParticipantSets = false;

					//获取原来的Outcome值
					var orginalOutcome = K2.ActivityInstance.DataFields["Outcome"].Value.ToString();
                 

					//获取节点上配置的【允许忽略加签队列的Action集合】（多个Action以逗号隔开）
					ignoringPeekPSetActions = K2.ActivityInstance.DataFields["_IGNORING_PEEKPSET_ACTIONS"].Value.ToString();

              
                    List<string> ignoringPeekPSetActionsList = new List<string>();

                    if(string.IsNullOrWhiteSpace(ignoringPeekPSetActions)!=null)
                        ignoringPeekPSetActionsList.AddRange((ignoringPeekPSetActions + string.Empty).Split(','));
                 
					//判断如果有加签队列，并且Outcome不在【允许忽略加签队列的Action集合】，则继续解析加签队列循环执行节点
					bool.TryParse(K2.ActivityInstance.DataFields["_HAS_UNPEEKED_PARTICIPANTSETS"].Value.ToString(), out hasUnpeekedParticipantSets);
                    if (hasUnpeekedParticipantSets && !ignoringPeekPSetActionsList.Contains(orginalOutcome))
						K2.ActivityInstance.DataFields["Outcome"].Value = "Signature";
            }

			private void SetSucceedingRuleFalse_ExecuteCode(object sender, EventArgs e)
			{
					K2.SucceedingRule = false;
			}

	}
}

