                        {
                            if (list[list.Count - 1] is XmlAttribute)
                            {
                                (list[list.Count - 1] as XmlAttribute).OwnerElement.ParentNode.RemoveChild((list[list.Count - 1] as XmlAttribute).OwnerElement);
