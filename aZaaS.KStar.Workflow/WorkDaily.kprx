                            break;
                        case K2Scope.Destination:
                            K2.ActivityInstanceDestination.XmlFields[xmlfield.Name].Value = xmlfield.XMLValue;
                            break;
                    }
                }
                #endregion

                this.XmlHelper = null;
            }
        }
