                        using (XmlTextWriter w = new XmlTextWriter(s, null))
                        {
                            w.Formatting = Formatting.Indented;
                            sampleGen.WriteXml(w);
                            s.Position = 0;

                            using (System.IO.StreamReader r = new System.IO.StreamReader(s))
