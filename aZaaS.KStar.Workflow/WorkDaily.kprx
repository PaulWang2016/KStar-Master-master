			private static string GetName(Project_362fffaa2b9d4bf1ad2708867581c80e.SucceedingRuleContext_ab08aab57523405ba6d3546c6b33b92a K2, int setIdx)
		        {
		            // Get the property that returns the result
		            string propName = string.Format("OutcomeName{0}", setIdx);
		            System.Reflection.PropertyInfo pi = K2.Configuration.GetType().GetProperty(propName);
		            if (pi == null)
		                return string.Empty;

		            // Invoke the get
		            System.Reflection.MethodInfo mi = pi.GetGetMethod();
		            if (mi == null)
		                return string.Empty;

		            object retval = mi.Invoke(K2.Configuration, null);

		            return Convert.ToString(retval);
		        }

		        private static bool GetRuleResult(Project_362fffaa2b9d4bf1ad2708867581c80e.SucceedingRuleContext_ab08aab57523405ba6d3546c6b33b92a K2, int setIdx)
		        {
		            // Get the property that returns the result
		            string propName = string.Format("OutcomeRule{0}", setIdx);
		            System.Reflection.PropertyInfo pi = K2.Configuration.GetType().GetProperty(propName);
		            if (pi == null)
		                return true;

		            // Invoke the get
		            System.Reflection.MethodInfo mi = pi.GetGetMethod();
		            if (mi == null)
		                return true;

		            object retval = mi.Invoke(K2.Configuration, null);

		            return Convert.ToBoolean(retval);
		        }
		}
