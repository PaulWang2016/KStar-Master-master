        private void ExpireActivityEscalationAction_ExecuteCode(object sender, EventArgs e)
        {
            var userName = string.Empty;
            var defaultAction = string.Empty;
            var serviceAccount = K2.StringTable["KSTAR_ENV_K2ServiceAccount"].ToString();
            var activityDefaultActionField = K2.StringTable["KSTAR_ENV_ActDefaultActionField"].ToString();

            if (K2.ActivityInstance.DataFields.Contains(activityDefaultActionField))
                defaultAction = K2.ActivityInstance.DataFields[activityDefaultActionField].Value.ToString();

            if (K2.ActivityInstance.Destinations.Count >= 1)
            {
                var activityDest = K2.ActivityInstance.Destinations[0];
                if (activityDest.Destinations.Count >= 1)
                    userName = activityDest.Destinations[0].Name.Split(':')[1];
            }

            if (!string.IsNullOrEmpty(serviceAccount)
                && userName.Equals(serviceAccount, StringComparison.OrdinalIgnoreCase))
            {
                K2.ActivityInstance.DataFields["Outcome"].Value = defaultAction;
                K2.ExpireActivity(K2.Configuration.ActivityName);
            }
        }
	}
